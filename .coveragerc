[run]
branch = True
omit =
    deletion_side_effects/migrations/*
    deletion_side_effects/tests/*
    deletion_side_effects/version.py
source = deletion_side_effects
[report]
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain if tests don't hit defensive assertion code:
    raise NotImplementedError
fail_under = 100
show_missing = 1